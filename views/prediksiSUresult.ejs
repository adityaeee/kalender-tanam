<div class="container mt-3">
  <div class="row center">
    <div class="col-md-4">
      <a href="/data/prediksi/suhuUdara" class="btn btn-dark">&laquo; Kembali</a>
    </div>
    <div class="col-md-6">
      <h2 class="fw-bold">PREDIKSI SUHU UDARA</h2>
    </div>
  </div>

  <div class="row justify-content-center">
    <div class="col-md-6 mt-5">
      <div class="card">
        <div class="card-header">
          <h3>Hasil Prediksi</h3>
        </div>
        <div class="card-body">
          <form method="post" action="/data/prediksi/suhuUdara">
            <% if (typeof errors != 'undefined') { %>
            <div class="alert alert-danger" role="alert">
              <ul>
                <% errors.forEach(error => { %>
                <li><%= error.msg %></li>
                <% }) %>
              </ul>
              <% } %>
            </div>

            <div class="mb-3">
              <label for="result" class="form-label">Hasil Prediksi</label>
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[0] %>" />
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[1] %>" />
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[2] %>" />
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[3] %>" />
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[4] %>" />
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[5] %>" />
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[6] %>" />
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[7] %>" />
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[8] %>" />
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[9] %>" />
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[10] %>" />
              <input type="text" class="form-control" id="result" name="result" value="<%= hasil[11] %>" />
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
